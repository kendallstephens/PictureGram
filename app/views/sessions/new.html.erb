<h2>Log In</h2>

<%= form_tag("/sessions", method: "post") do %>
  
    <%= label_tag :username %><div>
    <%= text_field_tag :username %><div>
    <%= label_tag :password %><div>
    <%= password_field_tag :password %><div>
    
    <%= submit_tag "Log-In", class: 'sign_in_button' %>
    <%end%>
   

