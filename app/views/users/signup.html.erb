<h1>Signup Form</h1>

<%= form_for User.create do |f| %>
   
    <%= f.label "Username" %>
    <%= f.text_field :username %>
   
    <%= f.label "Email" %>
    <%= f.text_field :email %>
    <%= f.label "Password " %>
    <%= f.password_field :password %>
   
    <%= f.submit %>
    <%end%>
   

    
   

   

